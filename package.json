{
  "name": "@profico/web",
  "version": "1.1.0",
  "config": {
    "docker_reg": "258375240157.dkr.ecr.eu-central-1.amazonaws.com",
    "docker_reg_dev": "docker.proficodev.com",
    "docker_image_name": "profico-web",
    "dir_dev": "/etc/profico-web"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next start",
    "start:local": "NODE_ENV=development next start",
    "lint:base": "eslint --ext .ts,.tsx ./",
    "lint:styles": "stylelint '**/*.scss' --fix --cache --cache-location=./node_modules/.cache/",
    "lint": "yarn lint:base --fix --cache --cache-location=./node_modules/.cache/ && yarn lint:styles",
    "docker:build": "docker build . -t $npm_package_config_docker_reg/$npm_package_config_docker_image_name:$npm_package_version -t $npm_package_config_docker_reg/$npm_package_config_docker_image_name:latest",
    "docker:push": "docker push $npm_package_config_docker_reg/$npm_package_config_docker_image_name:$npm_package_version",
    "docker:push:latest": "docker push $npm_package_config_docker_reg/$npm_package_config_docker_image_name:latest",
    "docker:build:dev": "docker build . -t $npm_package_config_docker_reg_dev/$npm_package_config_docker_image_name:$npm_package_version",
    "docker:run:dev": "cd $npm_package_config_dir_dev && docker-compose up -d",
    "version:set:dev": "echo \"VERSION=$npm_package_version\"> $npm_package_config_dir_dev/.env"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "classnames": "2.2.6",
    "date-fns": "2.14.0",
    "libphonenumber-js": "^1.9.11",
    "next": "latest",
    "nodemailer": "^6.4.17",
    "query-string": "^6.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.14.0",
    "react-use": "^15.3.6",
    "sass": "^1.30.0",
    "smoothscroll-polyfill": "^0.4.4",
    "use-query-params": "^1.1.9"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.14",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^17.0.0",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.1.3"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  }
}
